            <action id="MTMoonlightTheme" class="com.mallowigi.idea.actions.themes.MTMoonlightThemeAction"
                    text="Moonlight Theme" description="Moonlight theme"
                    icon="/icons/actions/themes/moonlight.svg">
            </action>
            <separator />
            <action id="MTCustomTheme" class="com.mallowigi.idea.actions.themes.MTCustomThemeAction"
                    text="Custom Theme" description="Material custom"
                    icon="/icons/actions/themes/custom.svg">
            </action>
            <action id="MTLightCustomTheme" class="com.mallowigi.idea.actions.themes.MTLightCustomThemeAction"
                    text="Light Custom Theme" description="Material light custom"
                    icon="/icons/actions/themes/light_custom.svg">
            </action>
            <action id="ChangeThemeGroup" class="com.mallowigi.idea.actions.themes.MTQuickChangeThemeAction"
                    text="External Themes" description="External themes" popup="true"
                    icon="/icons/actions/mt/themeSwitch.svg">
            </action>
        </group>

